{
	"type": "object",
	"title": "SFTP writer",
	"required": [
		"hostname",
		"port",
		"user",
		"#pass",
		"#private_key"
	],
	"properties": {
		"port": {
			"type": "integer",
			"default": 22,
			"title": "SFTP host port",
			"propertyOrder": 20
		},
		"user": {
			"type": "string",
			"title": "SFTP user name",
			"propertyOrder": 30
		},
		"#pass": {
			"type": "string",
			"title": "SFTP password",
			"format": "password",
			"propertyOrder": 40
		},
		"#private_key": {
			"type": "string",
			"format": "textarea",
			"description": "Your SSH private key, including the BEGIN RSA .. part. If used, the password is ignored.",
			"title": "SSH private key",
			"propertyOrder": 50
		},
		"hostname": {
			"type": "string",
			"title": "SFTP host URL",
			"propertyOrder": 10
		},
		"disabled_algorithms": {
			"type": "string",
			"title": "Algorithms to disable",
			"propertyOrder": 60
		}
	}
}
